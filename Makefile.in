SHELL=/bin/sh

default:: cadence

cadence::
	cd libcadence-embedded; make
	cd loader; make
	cd modules; make

install:: cadence
	cd ./libcadence-embedded; make install
	cd ./modules; make install
	#cd ./models; make install
	cd ./loader; make install


clean::
	cd libcadence-embedded; make clean
	cd loader; make clean
	cd modules; make clean
